{
    "manifest_version":2,
    "name": "Groupsv3",
    "version": "1.0",
    "permissions": [ "tabs","storage"],
    "content_scripts":[
        {
            "matches":[
                "<all_urls>"
            ],
            "js":["JS/content.js"]
        }
    ],
    "background":{
        "scripts":["JS/background.js"]
    },  
    "browser_action":{
        "default_icon":"/Images/get_started128.png",
        "default_popup":"HTML/groups.html",
        "default_title":"Launch Groups"     
        
    }
}